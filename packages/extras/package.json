{
  "name": "@threlte/extras",
  "version": "9.0.0",
  "author": "Grischa Erbe <hello@legrisch.com> (https://legrisch.com)",
  "license": "MIT",
  "description": "Utilities, abstractions and plugins for your Threlte apps",
  "scripts": {
    "dev": "vite dev",
    "package": "svelte-kit sync && svelte-package && node ./scripts/cleanupPackage.js && publint",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "cleanup": "rimraf node_modules .svelte-kit dist"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.7.7",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@threlte/core": "workspace:*",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/three": "^0.171.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@yushijinhun/three-minifier-rollup": "^0.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "publint": "^0.2.7",
    "rimraf": "^5.0.5",
    "svelte": "^5.14.4",
    "svelte-check": "^3.6.9",
    "svelte-preprocess": "^5.1.3",
    "svelte2tsx": "^0.7.6",
    "three": "^0.171.0",
    "tslib": "^2.6.2",
    "type-fest": "^4.15.0",
    "typescript": "^5.6.3",
    "vite": "^5.2.8"
  },
  "peerDependencies": {
    "svelte": ">=5",
    "three": ">=0.155"
  },
  "type": "module",
  "keywords": [
    "threlte",
    "svelte",
    "three",
    "three.js",
    "webgl",
    "webgpu",
    "webxr",
    "3d"
  ],
  "homepage": "https://threlte.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/threlte/threlte.git",
    "directory": "packages/extras"
  },
  "bugs": {
    "url": "https://github.com/threlte/threlte/issues"
  },
  "exports": {
    ".": {
      "types": "./src/lib/index.ts",
      "svelte": "./src/lib/index.ts"
    }
  },
  "types": "./src/lib/index.ts",
  "svelte": "./src/lib/index.ts",
  "publishOverrides": {
    "files": [
      "dist"
    ],
    "svelte": "./dist/index.js",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "svelte": "./dist/index.js"
      }
    },
    "types": "./dist/index.d.ts"
  },
  "dependencies": {
    "@threejs-kit/instanced-sprite-mesh": "^2.5.0",
    "camera-controls": "^2.9.0",
    "three-mesh-bvh": "^0.7.4",
    "three-perf": "github:jerzakm/three-perf#three-kit-threlte-fork",
    "three-viewport-gizmo": "^2.0.2",
    "troika-three-text": "^0.50.3"
  }
}
