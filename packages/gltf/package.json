  "devDependencies": {
    "chalk": "^5.6.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "rimraf": "^6.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
