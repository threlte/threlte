---
import type { Props } from '@astrojs/starlight/props'
import { Image } from 'astro:assets'
import Layout from '../layouts/LandingPage.astro'
import logoSrc from '$assets/logo/threlte-logo.png'
import LogoOnly from '$assets/logo/threlte-logo-only.png'
import Hero from '../components/landing-page/Hero/HeroWrapper.svelte'
import Socials from '@astrojs/starlight/components/SocialIcons.astro'
import Testimonials from '../components/landing-page/Testimonials/Testimonials.astro'
import FeatureSection from '../components/landing-page/FeatureSection.astro'
import Code from '$components/Code.astro'

// examples
import ScaleExampleRaw from '../components/landing-page/examples/ScaleExample.txt?raw'
import RapierExampleRaw from '../components/landing-page/examples/RapierExample.txt?raw'
import ExtrasExampleRaw from '../components/landing-page/examples/ExtrasExample.txt?raw'
import PluginsExample from '../components/landing-page/examples/PluginsExample.txt?raw'

import { URLs } from '$components/landing-page/URLs.ts'
---

<Layout>
	<header
		class="fixed left-0 right-0 top-0 z-10 flex h-20 flex-row items-center justify-between bg-[#0a0f19] px-6 [box-shadow:0_25px_50px_12px_rgb(10,15,25)]"
	>
		<div class="flex items-center">
			<Image src={logoSrc} width={222} alt="threlte logo" />
			<a href={URLs.docs.start} class="pl-12 text-[#fff9] no-underline">Documentation</a>
			<a href={URLs.services} class="pl-12 text-[#fff9] no-underline">Services</a>
		</div>
		<div class="social-icons flex gap-4">
			<Socials {...Astro.props} />
		</div>
	</header>
	<main class="mx-auto max-w-7xl px-6 pb-12 pt-20">
		<Hero client:load />

		<div class="flex flex-col gap-14 py-24">
			<div class="flex flex-col items-center gap-4">
				<div class="text-faded flex-1 text-center text-2xl font-light">
					A 3D framework for the web.<br />
					Built on top of
					<span class="font-bold text-orange">Svelte</span>
					and
					<span class="font-bold text-orange">Three.js</span>.
				</div>

				<div
					class="flex flex-row items-center justify-center gap-4 rounded-full border border-white/20 bg-white/5 px-2 py-2 shadow-xl"
