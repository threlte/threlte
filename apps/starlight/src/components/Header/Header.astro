import ThemeSelect from '@astrojs/starlight/components/ThemeSelect.astro'

// import Package from '../Icons/Package.astro'
import Book from '../Icons/Bookv2.astro'
// import Circles from '../Icons/Circles.astro'
---

<div class="header sl-flex relative">
  <div class="title-wrapper sl-flex">
    <SiteTitle {...Astro.props} />
  </div>
  <div class="sl-flex">
		hello
		<Book class="test" />
		hello
    <Search {...Astro.props} />
    <!-- <Search {...Astro.props} /> -->
    <!-- <Search {...Astro.props} />
    <Search {...Astro.props} /> -->

    <!-- <a
        href="/docs/examples/examples"
        class:list={['flex items-center gap-1 rounded-md text-faded hover:text-white group flex-row-reverse', activeUrlPathName.includes('/docs/examples') && activeLinkStyles]}
      >
        <Circles class="hidden h-[16px] lg:block" />Examples
      </a> -->
  </div>
  <div class="sl-hidden md:sl-flex right-group">
